<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yuxibing.mykotlindemo">

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".javacode.activity.CustomActivity"></activity>
        <activity android:name=".javacode.activity.ScrollViewActivity" />
        <activity android:name=".javacode.activity.ArtPracticeActivity" />
        <activity android:name=".javacode.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".javacode.activity.ViewGroupActivity" />
        <activity android:name=".javacode.activity.CustomViewActivity" />
        <activity android:name=".javacode.activity.CircleMenuActivity" />
        <activity android:name=".javacode.activity.SettingActivity" />
        <activity android:name=".kotlincode.DialogActivity" />
        <activity android:name=".javacode.activity.MallCategoryActivity" />
        <service
            android:name=".javacode.themedemo.DownLoadThemeService"
            android:exported="false" />
    </application>

</manifest>